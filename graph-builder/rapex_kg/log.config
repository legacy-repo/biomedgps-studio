{   
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
            "simple": {
                "format": "%(asctime)s - %(name)s:%(lineno)d - %(levelname)s - %(message)s"
            }
    },
    "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "NOTSET",
                "formatter": "simple",
                "stream": "ext://sys.stdout"
            }
    },
    "root":{
        "level": "DEBUG",
        "handlers": ["console"]
    }
}
